# Environment Configuration for CLOUD_AVAILABILITY_ZONE Network Automation
# Copy this file to .env and fill in your actual values

# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Model selection (default: claude-3-sonnet-20240229)
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Optional: API timeout (default: 30 seconds)
ANTHROPIC_TIMEOUT=30

# Optional: Enable debug logging for AI features
AI_DEBUG=false

# Network Automation Settings
NETWORK_TIMEOUT=30
CONCURRENT_WORKERS=6
LOG_LEVEL=INFO